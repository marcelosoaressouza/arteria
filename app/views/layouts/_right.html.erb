        <div class="span2" style="text-align: center; float: right;">
          <div class="well sidebar-nav">
            <ul class="nav nav-list">
              <li class="nav-header" style="color: #B12C2F;">Tags</li>
                <% if !@tags.nil? %>
                  <% if @tags.size > 0 %>
                    <% tag_cloud(@tags, %w(tag1 tag2 tag3 tag4)) do |tag, css_class| %>
                      <%= link_to tag.name, tagged_url(:tag => tag.name), :class => css_class %>
                    <% end %>
                  <% else %>
                    <b><%= t('No Items Registered') %></b>
                  <% end %>
                <% end %>

              <br/></br>

              <li class="nav-header" style="color: #B12C2F;">Parceiros (Feed)</li>
              <li>
                <% if @feed.size > 0 %>
                  <% @feed.each do |entry| %>
                    <% entry.each do |rss| %>
                      <% if rss.respond_to?('title') %>
                        <% rss.entries[0...2].each do |news| %>
                          <%= link_to news.title, news.url, :target => "_blank" %>
                        <% end %>
                      <% end %>
                    <% end %>
                  <% end %>
                <% else %>
                  <b><%= t('No RSS Found') %></b>
                <% end %>
              </li>
            </ul>
          </div>
        </div>
